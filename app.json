{
  "expo": {
    "name": "Tarefandoo",
    "slug": "tarefandoo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/tarefandoo-mini-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/TAREFANDOO.png",
      "resizeMode": "contain",
      "backgroundColor": "#8E7CC3"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
